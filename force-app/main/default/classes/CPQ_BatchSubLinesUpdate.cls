global class CPQ_BatchSubLinesUpdate implements Database.Batchable<sObject>,Database.AllowsCallouts {
    global String query;
    
    global Database.QueryLocator start(Database.BatchableContext BC) {
        query = ' select id, SBQQ__Contract__c, SBQQ__RenewalPrice__c, SBQQ__ChargeType__c, Status__c, SBQQ__SubscriptionType__c,'+
                ' SBQQ__TerminatedDate__c, Terms__c, SBQQ__StartDate__c, SBQQ__EndDate__c,SBQQ__Contract__r.TotalContractValue2__c'+
                ' FROM SBQQ__Subscription__c '+
                ' where Status__c != \'Off set\' AND Terms__c!=null AND SBQQ__ChargeType__c = \'Recurring\' AND SBQQ__Contract__r.TotalContractValue2__c=null';
        return Database.getQueryLocator(query);
        
    }
    
    global void execute(Database.BatchableContext BC, List<SBQQ__Subscription__c> sublst){ //List<Opportunity> oppList) {
        List<Contract> contUpdateList = new List<Contract>();
        Set<Id> conId = new Set<Id>();
        system.debug('==oppList=='+sublst);
        Decimal TotalVal = 0;
        Decimal TotalValwTerms = 0;
        for(SBQQ__Subscription__c sb : sublst){
            if(sb.SBQQ__RenewalPrice__c!=null){
                TotalVal += sb.SBQQ__RenewalPrice__c;
                TotalValwTerms += sb.SBQQ__RenewalPrice__c *sb.Terms__c;
                conId.add(sb.SBQQ__Contract__c);
            }
        }
        system.debug('==TotalVal=='+TotalVal);
        system.debug('==TotalValwTerms=='+TotalValwTerms);
        system.debug('==conId=='+conId);
        for(Contract cont: [select id,ContractTerm,TotalContractValue2__c,AssetsTotal__c,MonthlySubsTotal__c from contract where Id=: conId]){
                system.debug('==cont.AssetsTotal__c=='+cont.AssetsTotal__c);
                if(TotalVal != null && TotalVal != 0){
                    if(cont.TotalContractValue2__c == null){
                    cont.TotalContractValue2__c = TotalValwTerms;
                    cont.MonthlySubsTotal__c = TotalVal;
                    }
                }
                contUpdateList.add(cont);
        }
        system.debug('==contUpdateList=='+contUpdateList);
        if(!contUpdateList.isEmpty()){
            update contUpdateList;
        }
    }
    
